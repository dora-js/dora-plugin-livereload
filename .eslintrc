{
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb/base",
  "rules": {
    "no-console": [0],
    "no-underscore-dangle": [0],
    "no-useless-escape": [0]
  },
  "ecmaFeatures": {
    "generators": 1,
    "experimentalObjectRestSpread": true
  },
  "env": {
    "node": true,
    "mocha": true
  }
}
